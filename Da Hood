local plrs = game.Players
local lp = plrs.LocalPlayer
local char = lp.Character
local hmrp = char.HumanoidRootPart


local targ = plrs["Gskcbeos"]
local tchar = targ.Character
local thum = tchar.Humanoid
local thmrp = tchar.HumanoidRootPart
local vim = game:GetService("VirtualInputManager")


local function m1click() 
    vim:SendMouseButtonEvent(0,0,0,true,game,0)
    wait(2)
    vim:SendMouseButtonEvent(0,0,0,false,game,0)
end


if thum.health > 5 then
local oldcf = hmrp.CFrame
vim:SendMouseButtonEvent(0,0,0,true,game,0)
wait(1.5)
for i = 1,13 do
wait(0.02)
hmrp.CFrame = thmrp.CFrame * CFrame.Angles(0,90,0)
end
vim:SendMouseButtonEvent(0,0,0,false,game,0)
wait(1)
hmrp.CFrame = oldcf
end


